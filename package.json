{
  "name": "telegraf-session-store-ydb",
  "version": "1.0.3",
  "scripts": {
    "format": "prettier . --write",
    "build": "tsup",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.ts'"
  },
  "keywords": [
    "telegraf",
    "telegram",
    "ydb"
  ],
  "author": "Timur Guliamov",
  "license": "MIT",
  "description": "Session store for Telegraf using Yandex Database (YDB).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tplaymeow/telegraf-session-ydb.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "prettier": "3.4.2",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "telegraf": "^4.16.3",
    "ydb-sdk": "^5.8.0"
